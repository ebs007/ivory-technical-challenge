{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",

    "outDir": "./dist",
    "rootDir": "./src",

    "strict": true,

    "strictPropertyInitialization": false,

    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,

    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
